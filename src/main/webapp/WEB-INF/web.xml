<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>InsertData</servlet-name>
    <servlet-class>example.InsertData</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>Tokenize</servlet-name>
    <servlet-class>example.Tokenize</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>InsertData</servlet-name>
    <url-pattern>/create</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>Tokenize</servlet-name>
    <url-pattern>/intent</url-pattern>
  </servlet-mapping>

</web-app>
